{
  "name": "vsutils",
  "displayName": "vsUtils",
  "description": "",
  "version": "0.0.1",
  "icon": "src/media/icon.svg",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vsutils-sidebar-view",
          "title": "utils",
          "icon": "src/media/icon.svg"
        }
      ]
    },
    "views": {
      "vsutils-sidebar-view": [
        {
          "type": "webview",
          "id": "vsutils-sidebar",
          "name": "utils view",
          "icon": "src/media/icon.svg",
          "contextualTitle": "vsUtilsSidebar"
        }
      ]
    },
    "commands": [
      {
        "command": "vsutils.israelId",
        "title": "generate israel Id",
        "category": "util.generate"
      },
      {
        "command": "vsutils.fc-ts",
        "title": "insert react fc with interface",
        "category": "util.generate"
      },
      {
        "command": "vsutils.example",
        "title": "example",
        "category": "util.example"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "vscode-test": "^1.6.1"
  }
}
